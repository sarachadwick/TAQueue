<h1>Get Help</h1>

<div>
  <div>
    <%= form_for(Student.new(), url: get_help_path) do |f| %>
      <%= render 'shared/error_messages' %>

      <%= f.label :reason %>
      <%= f.text_field :reason, class: 'form-control' %>
      
      <%= f.submit "Sign In For Help", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
